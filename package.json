{
  "name": "user-manager",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^16.14.0 || >=18.10.0"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "ng serve",
    "ng": "ng",
    "start": "yarn dev",
    "build": "ng build",
    "watch": "ng build --watch",
    "test": "ng test",
    "db:start": "npx prisma init --datasource-provider sqlite",
    "db:migrate": "npx prisma migrate dev",
    "db:pull": "npx prisma db pull",
    "db:generate": "npx prisma generate",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma migrate reset && yarn db:seed && yarn db:generate",
    "setup": "yarn && yarn db:reset && yarn dev",
    "project:reset": "rm -rf node_modules && rm -rf dist && yarn setup"
  },
  "dependencies": {
    "@analogjs/content": "^0.2.0-beta.15",
    "@analogjs/router": "^0.2.0-beta.15",
    "@angular-devkit/core": "14.0.0",
    "@angular-devkit/schematics": "14.0.0",
    "@angular/animations": "^16.0.0",
    "@angular/common": "^16.0.0",
    "@angular/compiler": "^16.0.0",
    "@angular/core": "^16.0.0",
    "@angular/forms": "^16.0.0",
    "@angular/platform-browser": "^16.0.0",
    "@angular/platform-browser-dynamic": "^16.0.0",
    "@angular/platform-server": "^16.0.0",
    "@angular/router": "^16.0.0",
    "@ngrx/effects": "^16.0.1",
    "@ngrx/entity": "^16.0.1",
    "@ngrx/router-store": "^16.0.1",
    "@ngrx/store": "^16.0.1",
    "@ngrx/store-devtools": "^16.0.1",
    "@ngtools/webpack": "^16.0.5",
    "@nx/angular": "^16.0.0",
    "@prisma/client": "^4.15.0",
    "@schematics/angular": "14.0.0",
    "@types/node": "^20.3.0",
    "eslint": "^8.42.0",
    "front-matter": "^4.0.2",
    "marked": "^5.0.2",
    "prismjs": "^1.29.0",
    "rxjs": "~7.5.6",
    "tailwind-merge": "^1.13.1",
    "ts-node": "10",
    "tslib": "^2.4.0",
    "webpack": "^5.54.0",
    "zod": "^3.21.4",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@analogjs/platform": "^0.2.0-beta.15",
    "@angular-devkit/build-angular": "^16.0.0",
    "@angular/cli": "^16.0.0",
    "@angular/compiler-cli": "^16.0.0",
    "@nx/vite": "^16.0.0",
    "autoprefixer": "^10.4.14",
    "jsdom": "^20.0.0",
    "nx": "^16.0.0",
    "postcss": "^8.4.21",
    "prisma": "^4.15.0",
    "tailwindcss": "^3.3.1",
    "typescript": "~5.0.2",
    "vite": "^4.0.3",
    "vitest": "^0.31.0"
  }
}
